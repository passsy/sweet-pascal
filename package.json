{
  "name": "sweet-pascal",
  "displayName": "Sweet Pascal",
  "description": "A dark theme with pastel colors, inspired by Mario Kart 8 Deluxe's Sweet Sweet Canyon track.",
  "version": "0.0.3",
  "author": "Pascal Welsch",
  "publisher": "PascalWelsch",
  "repository": {
    "type": "git",
    "url": "https://github.com/passsy/sweet-pascal"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dark",
    "pastel",
    "sweet",
    "theme"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Sweet Pascal",
        "uiTheme": "vs-dark",
        "path": "./themes/sweet-pascal-color-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.26.0",
    "typescript": "^5.0.0"
  },
  "license": "MIT"
}
