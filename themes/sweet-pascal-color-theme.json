{
  "name": "Sweet Pascal",
  "type": "dark",
  "colors": {
    "editor.background": "#2b2b2b",
    "editor.foreground": "#e5e5e5",
    "editorGutter.background": "#222424",
    "editorLineNumber.foreground": "#4a4a4a",
    "editor.lineHighlightBackground": "#171818",
    "editorCursor.foreground": "#777d81",
    "editor.selectionBackground": "#1b3139",
    "editorIndentGuide.background1": "#3d3d3d",
    "editorIndentGuide.activeBackground1": "#cccccf",
    "editorWhitespace.foreground": "#ff2457",
    "editorRuler.foreground": "#616161",
    "editorOverviewRuler.border": "#27292a",
    "peekViewEditor.background": "#222424",
    "peekViewResult.background": "#171818",
    "peekViewTitle.background": "#1b3139",
    "list.highlightForeground": "#98b18c",
    "list.activeSelectionBackground": "#1b3139",
    "list.activeSelectionForeground": "#e5e5e5",
    "list.inactiveSelectionBackground": "#222424",
    "list.hoverBackground": "#171818",
    "activityBar.background": "#222424",
    "sideBar.background": "#222424",
    "statusBar.background": "#222424",
    "statusBar.noFolderBackground": "#222424",
    "statusBar.debuggingBackground": "#bf9f4d",
    "tab.activeBackground": "#171818",
    "tab.inactiveBackground": "#222424",
    "editorGroupHeader.tabsBackground": "#222424",
    "notificationCenterHeader.background": "#5c5c42",
    "notifications.background": "#5c5c42",
    "scrollbarSlider.background": "#3d3d3d80",
    "scrollbarSlider.hoverBackground": "#4a4a4a80",
    "scrollbarSlider.activeBackground": "#777d8180",
    "diffEditor.insertedTextBackground": "#0c520e40",
    "editorGutter.addedBackground": "#0c520e",
    "diffEditor.removedTextBackground": "#da091340",
    "editorGutter.deletedBackground": "#da0913",
    "editorGutter.modifiedBackground": "#bf9f4d",
    "terminal.background": "#2b2b2b",
    "terminal.foreground": "#e5e5e5",
    "terminal.ansiBlack": "#1c1a1a",
    "terminal.ansiBrightBlack": "#898989",
    "terminal.ansiRed": "#ea5e5e",
    "terminal.ansiBrightRed": "#ff6b68",
    "terminal.ansiGreen": "#5c962c",
    "terminal.ansiBrightGreen": "#7bea96",
    "terminal.ansiYellow": "#a68a0d",
    "terminal.ansiBrightYellow": "#e4e24a",
    "terminal.ansiBlue": "#3993d4",
    "terminal.ansiBrightBlue": "#a4c2ff",
    "terminal.ansiMagenta": "#983498",
    "terminal.ansiBrightMagenta": "#ff92f1",
    "terminal.ansiCyan": "#06969",
    "terminal.ansiBrightCyan": "#8dd2c5",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightWhite": "#e5e5e5"
  },
  "tokenColors": [
    {
      "name": "Default Text",
      "scope": ["source", "text", "variable.other.readwrite"],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8b8b8b"
      }
    },
    {
      "name": "Documentation Comment",
      "scope": ["comment.block.documentation", "comment.line.documentation"],
      "settings": {
        "foreground": "#9c7b7b"
      }
    },
    {
      "name": "Documentation Comment Tag",
      "scope": [
        "comment.block.documentation storage.type",
        "comment.block.documentation variable.parameter",
        "comment.block.documentation keyword"
      ],
      "settings": {
        "foreground": "#8a653b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#e5b597",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Preprocessor/Annotation",
      "scope": [
        "meta.preprocessor",
        "keyword.control.import",
        "keyword.control.export",
        "entity.name.tag.documentation",
        "meta.annotation",
        "entity.name.annotation",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#b1b8b1"
      }
    },
    {
      "name": "Variable, Parameter",
      "scope": ["variable", "meta.definition.variable.js"],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#c0cdde"
      }
    },
    {
      "name": "Reassigned Variable/Parameter",
      "scope": [],
      "settings": {
        "foreground": "#c0cdde"
      }
    },
    {
      "name": "Constants & Special Variables",
      "scope": [
        "constant.language",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#9ec2bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Static/Class Variable",
      "scope": [
        "variable.other.constant",
        "variable.other.readwrite.static",
        "entity.name.variable.static"
      ],
      "settings": {
        "foreground": "#eedab8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Label",
      "scope": ["entity.name.label"],
      "settings": {
        "foreground": "#d7d687"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#d297b0"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#bb700f"
      }
    },
    {
      "name": "Invalid String Escape",
      "scope": ["invalid.illegal.string"],
      "settings": {
        "foreground": "#d72d2d"
      }
    },
    {
      "name": "Regular Expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#73a0ff"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#8dd2c5"
      }
    },
    {
      "name": "Class Name",
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#98b18c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inherited Class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#98b18c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Interface Name",
      "scope": ["entity.name.interface"],
      "settings": {
        "foreground": "#bfbc86",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Enum Name",
      "scope": ["entity.name.enum", "support.enum"],
      "settings": {
        "foreground": "#b5b882",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Enum Member",
      "scope": ["constant.other.enum"],
      "settings": {
        "foreground": "#d99b79"
      }
    },
    {
      "name": "Type Parameter",
      "scope": ["entity.name.type.parameter", "variable.type.parameter"],
      "settings": {
        "foreground": "#cd7d46"
      }
    },
    {
      "name": "Function/Method Name",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Constructor Call",
      "scope": [
        "meta.function-call variable.function",
        "entity.name.function.constructor"
      ],
      "settings": {
        "foreground": "#98b18c"
      }
    },
    {
      "name": "Static Method",
      "scope": ["entity.name.function.static", "support.function.static"],
      "settings": {
        "foreground": "#eedab8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML/XML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#e5b597",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML/XML Attribute Name",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "name": "HTML/XML Attribute Value",
      "scope": ["string.quoted.double", "string.quoted.single"],
      "settings": {
        "foreground": "#d297b0"
      }
    },
    {
      "name": "HTML/XML Entity",
      "scope": ["constant.character.entity"],
      "settings": {
        "foreground": "#b8b8ff"
      }
    },
    {
      "name": "Bash External Command",
      "scope": ["support.function.external.bash"],
      "settings": {
        "foreground": "#98b18c"
      }
    },
    {
      "name": "Dart Instance Field",
      "scope": ["variable.other.readwrite.instance.dart"],
      "settings": {
        "foreground": "#a4c2ff"
      }
    },
    {
      "name": "Dart ternary operator",
      "scope": ["keyword.operator.ternary.dart"],
      "settings": {
        "foreground": "#e5e5e5"
      }
    },
    {
      "name": "Kotlin Named Argument",
      "scope": ["variable.parameter.named.kotlin"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Kotlin Dynamic Call",
      "scope": ["meta.dynamic.call.kotlin"],
      "settings": {
        "foreground": "#ff92f1"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f34c4c"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#e5e5e5",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
